@using Whiskey.ZeroStore.ERP.Models
@model StoreDeposit

<div class="form-group">
    <label class="control-label col-md-2">
		@Html.DisplayNameFor(m=>m.StoreId) :
    </label>
    <div class="col-md-10">
		@Model.Store.StoreName
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
		@Html.DisplayNameFor(m=>m.DepositType) :
    </label>
    <div class="col-md-10">
        @Html.Raw(Model.DepositType.ToString())
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
		@Html.DisplayNameFor(m=>m.Price) :
    </label>
    <div class="col-md-10">
		@Model.Price
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
		@Html.DisplayNameFor(m=>m.Cash) :
    </label>
    <div class="col-md-10">
		@Model.Cash
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
        @Html.DisplayNameFor(m => m.Card) :
    </label>
    <div class="col-md-10">
        @Model.Card
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
        @Html.DisplayNameFor(m => m.Remit) :
    </label>
    <div class="col-md-10">
        @Model.Remit
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
		@Html.DisplayNameFor(m=>m.Notes) :
    </label>
    <div class="col-md-10">
		@Model.Notes
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
		@Html.DisplayNameFor(m=>m.UpdatedTime) :
    </label>
    <div class="col-md-10">
		@Model.UpdatedTime
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
		@Html.DisplayNameFor(m=>m.CreatedTime) :
    </label>
    <div class="col-md-10">
		@Model.CreatedTime
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
		@Html.DisplayNameFor(m=>m.IsEnabled) :
    </label>
    <div class="col-md-10">
        @UiHelper.showTrueFalse(Model.IsEnabled)
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
		@Html.DisplayNameFor(m=>m.IsDeleted) :
    </label>
    <div class="col-md-10">
        @UiHelper.showTrueFalse(Model.IsDeleted)
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
		操作人员 :
    </label>
    <div class="col-md-10">
        @UiHelper.showOperator(Model.Operator)
    </div>
</div>
