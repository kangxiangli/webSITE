@using Whiskey.ZeroStore.ERP.Models
@model MemberImportRecord

@*<link rel="stylesheet" href="/content/styles/jquery/jquery.zTreeStyle.css" />*@

<ul id="product" class="nav nav-tabs">
    <li class="active">
        <a href="#base" data-toggle="tab">基础信息</a>

    </li>
    @*<li>
            <a href="#info" data-toggle="tab">试题信息</a>
        </li>*@

</ul>
<div class="tab-content">
    <div class="tab-pane fade active in " id="base">
        <div class="form-group">
            <label class="control-label col-md-3">
                @Html.DisplayNameFor(m => m.TotalCount) :
            </label>
            <div class="col-md-7">
                @Html.DisplayFor(m => m.TotalCount, new { @class = "form-control" })
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">
                @Html.DisplayNameFor(m => m.SuccessCount) :
            </label>
            <div class="col-md-7">
                @Html.DisplayFor(m => m.SuccessCount, new { @class = "form-control" })
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">
                @Html.DisplayNameFor(m => m.Store.StoreName) :
            </label>
            <div class="col-md-7">
                @if (!Model.StoreId.HasValue)
                {
                    <span>无</span>
                }
                else
                {
                    @Html.DisplayFor(x => x.Store.StoreName, new { @class = "form-control" })
                }

            </div>
        </div>

        <div class="form-group">
            <label class="control-label col-md-3">
                @Html.DisplayNameFor(m => m.CreateStartDate) :
            </label>
            <div class="col-md-7">
                @if (!Model.CreateStartDate.HasValue)
                {
                    <span>无</span>

                }
                else
                {

                    @Html.DisplayFor(m => m.CreateStartDate, new { @class = "form-control" })
                }
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">
                @Html.DisplayNameFor(m => m.CreateEndtDate) :
            </label>
            <div class="col-md-7">
                @if (!Model.CreateEndtDate.HasValue)
                {
                    <span>无</span>
                }
                else
                {
                    @Html.DisplayFor(m => m.CreateEndtDate, new { @class = "form-control" })
                }
            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-md-3">
                @Html.DisplayNameFor(m => m.CreatedTime) :
            </label>
            <div class="col-md-7">
                @Html.DisplayFor(m => m.CreatedTime, new { @class = "form-control" })
            </div>
        </div>


        <div class="form-group">
            <label class="control-label col-md-3">
                操作人:
            </label>
            <div class="col-md-7">
                @Html.DisplayFor(m => m.Operator.Member.MemberName, new { @class = "form-control" })
            </div>
        </div>


    </div>
</div>
