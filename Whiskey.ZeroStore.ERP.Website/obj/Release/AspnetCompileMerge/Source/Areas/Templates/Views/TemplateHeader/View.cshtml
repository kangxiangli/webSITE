@using Whiskey.ZeroStore.ERP.Models
@model TemplateHeader

<div class="form-group">
    <label class="control-label col-md-2">
        名称：
    </label>
    <div class="col-md-10">
        @Model.HeaderName
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
        发布人：
    </label>
    <div class="col-md-10">
        @Model.HeaderPublisher
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
        更新时间：
    </label>
    <div class="col-md-10">
        @Model.UpdatedTime
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
        创建时间：
    </label>
    <div class="col-md-10">
        @Model.CreatedTime
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
        内容：
    </label>
    <div class="col-md-10">
        @Html.TextAreaFor(m => m.HeaderHtml, new { @readonly = "true", @style = "width:100%", @rows = "4" })
    </div>
</div>
