
<div class="tab-content" style="min-height:400px;">
    <div class="tab-pane fade active in " id="base">


        <div class="form-group">

            <label class="control-label col-md-4">商品款号：</label>
            <div class="col-md-4 ">
                <span>@ViewBag.BigNum</span>

            </div>

        </div>
        <div class="form-group">

            <label class="control-label col-md-4">原始款号：</label>
            <div class="col-md-4 ">
                <span>@ViewBag.OrigNum</span>

            </div>

        </div>

        <div class="form-group">
            <label class="control-label col-md-4">总计：</label>
            <div class="col-md-4 ">
                <span>@ViewBag.Cou</span>
            </div>
        </div>
        
        <div class="form-group">

            <label class="control-label col-md-4">商品品牌：</label>
            <div class="col-md-4">
                @ViewBag.BrandNam
            </div>

        </div>
        <div class="form-group">
            <label class="control-label col-md-4">商品款式：</label>
            <div class="col-md-4">
                @ViewBag.CategoNam

            </div>
        </div>
        <div class="form-group">
            <label class="control-label col-md-4">缩略图：</label>
            <div class="col-md-4">
                <div class="col-md-4">
                    <img style="border: 1px solid #eaeaea" src="@ViewBag.ThumPath" />
                </div>
            </div>
        </div>
       
    </div>
</div>
