




@using Whiskey.ZeroStore.ERP.Transfers
@model RestoredDto
<div class="form-group">
    <label class="control-label col-md-2">
        @Html.DisplayNameFor(m=>m.DeliverId) :
    </label>
    <div class="col-md-10">
        @Html.TextBoxFor(m=> m.DeliverId, new { @class = "form-control" })
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
        @Html.DisplayNameFor(m=>m.ReceiverId) :
    </label>
    <div class="col-md-10">
        @Html.TextBoxFor(m=> m.ReceiverId, new { @class = "form-control" })
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
        @Html.DisplayNameFor(m=>m.Status) :
    </label>
    <div class="col-md-10">
        @Html.TextBoxFor(m=> m.Status, new { @class = "form-control" })
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
        @Html.DisplayNameFor(m=>m.RestoreNumber) :
    </label>
    <div class="col-md-10">
        @Html.TextBoxFor(m=> m.RestoreNumber, new { @class = "form-control" })
    </div>
</div>
@Html.HiddenFor(m=> m.Id)
