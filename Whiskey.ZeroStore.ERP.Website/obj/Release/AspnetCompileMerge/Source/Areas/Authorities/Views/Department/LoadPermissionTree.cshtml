<link href="~/Content/Styles/PermissionTree/permissionTree.css" rel="stylesheet" />
<script src="~/Content/Scripts/PermissionTree/PermissionTree.js"></script>
<div>
    <div class="tree" style="height:600px;"></div>
</div>
<script>
    $(function () {
        var id = $("#hid-id").val();

        $.post("/Authorities/Department/GetTree?did=" + id, function (data) {
            $(".tree").loadTree(data);
        });
    });
</script>
