




@using Whiskey.ZeroStore.ERP.Models
@model Module

<div class="form-group">
    <label class="control-label col-md-4">
		@Html.DisplayNameFor(m=>m.ParentId) :
    </label>
    <div class="col-md-8">
		@if (Model.Parent == null)
        {
            <span>无</span>
        }
        else { 
        @Model.Parent.ModuleName
        }
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-4">
        @Html.DisplayNameFor(m => m.ModuleType) :
    </label>
    <div class="col-md-8">
        @Html.Raw(Model.ModuleType == 0 ? "一级菜单" : "二级菜单")
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-4">
		@Html.DisplayNameFor(m=>m.ModuleName) :
