                });
            } else {
                $.whiskey.web.alert({
                    type: "info",
                    content: "请至少选择一条数据！",
                    callback: function () {
                    }
                });
            }
        });


        $("#Search").on("click", function () {
            $.whiskey.datatable.reset(false);
        });

        $("#Clear").on("click", function () {
            $.whiskey.web.clearForm(".form-search");
