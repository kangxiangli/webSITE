                    notes: "提示：将数据从回收站移动至正常数据列表里",
                    actionUrl: "@Url.Action("Recovery")",
                    params: list,
                    complete: function () {
                        $.whiskey.datatable.reset(false);
                    }
                });
            } else {
                $.whiskey.web.alert({
                    type: "info",
                    content: "请至少选择一条数据！",
                    callback: function () {
                    }
                });
            }
        });


        $("#DeleteAll").on("click", function () {
            var list = $.whiskey.web.getIdByChecked(".table-list td input[type=checkbox]");
