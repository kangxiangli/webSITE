
@using Whiskey.ZeroStore.ERP.Models
@model ProductCrowd

<style>

</style>

<div class="form-group">
    <label class="control-label col-md-2">
        @Html.DisplayNameFor(m => m.CrowdName) :
    </label>
    <div class="col-md-10">
        @Model.CrowdName
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
        @Html.DisplayNameFor(m => m.CrowdCode) :
    </label>
    <div class="col-md-10">
        @Model.CrowdCode
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
        @Html.DisplayNameFor(m => m.IconPath) :
    </label>
    <div class="col-md-1">
        <div style="display:block;width:80px;height:80px;overflow:hidden;border:1px solid #eaeaea;margin:0 auto 0 auto;">
            <img src="@Model.IconPath" style="margin:2px;max-width:74px;" />
        </div>
    </div>
</div>

<div class="form-group">
    <label class="control-label col-md-2">
        @Html.DisplayNameFor(m => m.Description) :
    </label>
    <div class="col-md-10">
        @Model.Description
    </div>
</div>

<div class="form-group">
    <label class="control-label col-md-2">
        @Html.DisplayNameFor(m => m.UpdatedTime) :
    </label>
    <div class="col-md-10">
        @Model.UpdatedTime
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
        @Html.DisplayNameFor(m => m.CreatedTime) :
    </label>
    <div class="col-md-10">
        @Model.CreatedTime
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
        @Html.DisplayNameFor(m => m.IsEnabled) :
    </label>
    <div class="col-md-10">
        @Html.Raw(Model.IsEnabled == true ? "是" : "否")
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
        @Html.DisplayNameFor(m => m.IsDeleted) :
    </label>
    <div class="col-md-10">
        @Html.Raw(Model.IsDeleted == true ? "是" : "否")
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
        操作人员 :
    </label>
    <div class="col-md-10">
        @Html.Raw(Model.Operator != null ? Model.Operator.Member.MemberName : "")
    </div>
</div>

<script>
    $(function () {


    });
</script>

