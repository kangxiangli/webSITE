
@using Whiskey.ZeroStore.ERP.Transfers
@model StorageDto

<div class="form-group">
    <label class="control-label col-md-3">
		@Html.DisplayNameFor(m=>m.StoreId) :
    </label>
    <div class="col-md-7">
		@ViewBag.storeName
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-3">
        @Html.DisplayNameFor(m=>m.StorageType) :
    </label>
    <div class="col-md-7">
        @Html.Raw(Model.StorageType==0?"线上仓库":"线下仓库")
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-3">
        @Html.DisplayNameFor(m => m.StorageCategory) :
    </label>
    <div class="col-md-7">
        @Html.Raw(Model.StorageCategory == 0 ? "商品仓" : "设备舱")
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-3">
		@Html.DisplayNameFor(m=>m.StorageName) :
    </label>
