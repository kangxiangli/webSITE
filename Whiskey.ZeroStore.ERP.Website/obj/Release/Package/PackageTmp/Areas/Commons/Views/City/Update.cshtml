




@using Whiskey.ZeroStore.ERP.Transfers
@model CityDto
<div class="form-group">
    <label class="control-label col-md-2">
        @Html.DisplayNameFor(m=>m.CityName) :
    </label>
    <div class="col-md-10">
        @Html.TextBoxFor(m=> m.CityName, new { @class = "form-control" })
    </div>
</div>
<div class="form-group">
    <label class="control-label col-md-2">
        @Html.DisplayNameFor(m=>m.ProvinceId) :
    </label>
    <div class="col-md-10">
        @Html.TextBoxFor(m=> m.ProvinceId, new { @class = "form-control" })
    </div>
</div>
@Html.HiddenFor(m=> m.Id)
