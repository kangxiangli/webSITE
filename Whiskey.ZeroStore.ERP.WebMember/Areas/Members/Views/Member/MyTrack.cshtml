<div class="rt-nodataContent notHaveAppoint">
	还未浏览任何商品，<a href="/Home/Online">浏览商品></a>
</div>
<div class="rt-nodataContent dontHaveStore">
	还未选择归属店铺，快去 个人信息->归属店铺 选择归属店铺吧~
</div>
<div class="tableTitle">我的足迹</div>
<div class="rt-content">

	<div class="swiper-container">
		<div class="swiper-wrapper" id="swiperBox">
		</div>
		<!--<div class="swiper-button-prev swiper-button-white"></div>
		<div class="swiper-button-next swiper-button-white"></div>-->
	</div>
	<div class="mouse" ><img title="滚动鼠标滚轮,左右切换预约单" src="data:image/png;base64,"/></div>
</div>
<link rel="stylesheet" href="/Content/Styles/swiper/swiper.css" />
<script src="/Content/Scripts/swiper/swiper.min.js"></script>
<script>
	$(function(){
		
	    var isPosting = false;
	    var PageIndex = 1;
		var PageSize = 10;
		var Stat = null;
		var AllCount = 99999;
		var HasAppoint = true;
		
		var mySwiper = new Swiper('.swiper-container', {
			prevButton:'.swiper-button-prev',
			nextButton:'.swiper-button-next',
			effect: 'coverflow',
			slidesPerView: 4,
			centeredSlides: true,
			onTransitionStart :function(swiper){
				var activeIndex = swiper.activeIndex;
				if(($('.swiper-slide').length - activeIndex) < 5 && !isPosting){
					getAppointment(isPosting);
				}
			},
			coverflow: {
				rotate: 35,
				stretch: 10,
				depth: 50,
				modifier: 1,
				slideShadows: true
			},
			observer: true,
			observeParents: true,
			mousewheelControl : true,
		})
		$(document).on('click','.swiper-slide-prev',function(){
			mySwiper.slidePrev();
		})
		$(document).on('click','.swiper-slide-next',function(){
			mySwiper.slideNext();
		})
		//----------------------------------------------------------获取预约单

		function getAppointment(isPosting){
			if(PageIndex > Math.ceil(AllCount/PageSize)  ){
				return;
			}
			
			$.ajax({
				url:'/Members/Member/GetBehaviourRecord',
				type:'post',
				beforeSend:function(){
					isPosting=true;
				},
				completed:function(){
					isPosting=false;
				},
				data:{
					PageIndex:PageIndex,
					PageSize:PageSize,
					Stat:Stat
				},
				success:function(data){
					
					if(data.ResultType == 3){
						var html = '';
						AllCount = data.Data.TotalCount;
						if(AllCount > 0){
							$(data.Data.List).each(function(i,item){				
								html += '<div class="swiper-slide"><div class="AppointmentTitle"><div class="AppointmentIndex">' + item.BigProdNum +								
								'</div><div class="AppointmentOpstion">'+
								'<a target="_black" href="/products/product/detail?BigProdNum='+ item.BigProdNum +'" class="see"></a>' +
								'</div></div><div class="clearfix AppointmentShow"><div class="AppointmentImage">' +
								'<img src="'+ item.ThumbnailPath +'" onerror="imgError(this)" /></div><div class="Appointmentdes">' +
								'<div>品牌：'+ item.BrandName +'</div>' +
								'<div>名称：'+ item.ProductName +'</div>' +
								'<div>价格：'+ item.TagPrice +'</div><div title='+ item.PageViews +'>浏览量：' 
								if(item.PageViews >= 1){
									if(item.PageViews > 5){
										item.PageViews = 5
									}
									for(var a = 1; a <= item.PageViews ; a++ ){
										html+= '<img class="feet" src="/Content/Images/feet.png">'
									}
									if(item.PageViews >= 5){
										html+= '<img class="feet" src="/Content/Images/more.png">'
									}
									
								}							
								html += '</div></div></div></div>'
							})
						}
						
						$('#swiperBox').append(html)
						if(PageIndex == 1 ){
							mySwiper.slideTo(0, 1000, false);//切换到第一个slide，速度为1秒
						}
					}else if( data.ResultType == 4){
						$('.dontHaveStore').show()
					}
					
					if(HasAppoint && AllCount > 0 ){
						$('.rt-content').show();
					}else if(HasAppoint && AllCount == 0){
						$('.notHaveAppoint').show()
					}
					HasAppoint = false;
					PageIndex++;
				}
			})
		}
		
		//-------------------------------------------筛选预约单状态
		$(document).click(function(){
			$('.drop-down').slideUp()
			$('.triangle').removeClass('triangleUp');
		})
		$('.selestStatus').click(function(){
			$('.drop-down').toggle();
			if($('.triangle').hasClass('triangleUp')){
				
				$('.triangle').removeClass('triangleUp');
			}else{
				$('.triangle').addClass('triangleUp');
			}
			return false;
		})
		
		$('.drop-down>div').click(function(){
			$('.selestStatus>b').text($(this).text())
			$('#swiperBox').empty()
			PageIndex = 1;
			AllCount = 99999;
			Stat = $(this).attr('data-stat');
			getAppointment()
		})
	})
	
	function imgError(image) {
        $(image).attr('src', '/content/images/shoping/errorImage.png');
    }
</script>