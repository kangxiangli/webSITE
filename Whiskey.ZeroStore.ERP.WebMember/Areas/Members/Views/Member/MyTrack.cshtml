	}
	.tableHeader{
		width: 50%;
		margin-left: 25%;
		height: 10%;
	}
	.oneHeader{
		float: left;
		width: 20%;
		text-align: center;
		color: #FFF;
		line-height: 30px;
   		margin: 10px 0;
