<!doctype html>
@{ Layout = null; }
@using Whiskey.ZeroStore.ERP.Models.Entities
@using Whiskey.Utility.Extensions;
@using Whiskey.Utility.Helper;
@model ProductOriginNumber

@if (Model.IsNotNull())
{
    <title>零时尚 - @Model.ProductName</title>

    var WebUrl = ConfigurationHelper.WebUrl;
    var listpro = Model.Products.Where(w => w.IsEnabled && !w.IsDeleted).ToList();

    var listColorSizes = from s in listpro
                         group s by new { s.ColorId, s.Color.ColorName, s.Color.IconPath } into g
                         select new
                         {
                             ColorId = g.Key.ColorId,
                             g.Key.ColorName,
                             ColorIcon = g.Key.IconPath,
                             Sizes = g.Select(ss => new { ss.SizeId, ss.Size.SizeName, ss.ProductNumber, ProductId = ss.Id })
                         };

    var listcolorsizes = listColorSizes.Select(s => new { s.ColorId, Sizes = s.Sizes }).ToJsonString();
    <link rel="stylesheet" href="/Content/Styles/product/index.css" />
    <link rel="stylesheet" href="/Content/Styles/swiper/swiper.css" />

    <div class="rt-content">
        <div class="product-part1">
            <div class="productTitle"><img src="/Content/Images/product/productTitle.png" /></div>
        </div>
        <div class="clearfix product-show-box">
            <div class="product-show-leftbox">
                <img class="main-product-show" src="@WebUrl@Model.OriginalPath" />
            </div>
            <div class="product-show-rightbox">
                <div class="product-attribute procuct-name">@Model.ProductName</div>
                <div class="product-attribute product-color">
                    <span class="product-attribute-title">颜色</span>

                    @{ var tempIndex = 0;}
                    @foreach (var item in listColorSizes)
                    {
                        var colorimg = WebUrl + item.ColorIcon;

                        var stractive = tempIndex > 0 ? "" : "product-color-image-active";

                        <img class="product-color-image @stractive" src="@colorimg" title="@item.ColorName" data-ColorId="@item.ColorId" />
                        ++tempIndex;
                    }

                </div>
                <div class="product-attribute product-sizeList">
                    <span class="product-attribute-title">尺码</span>

                    @{ tempIndex = 0;}
                    @foreach (var item in listColorSizes.FirstOrDefault().Sizes)
                    {
                        var stractive = tempIndex > 0 ? "" : "product-osize-avtive";
                        <span class="product-osize @stractive" data-SizeId="@item.SizeId" data-ProductId="@item.ProductId" data-ProductNumber="@item.ProductNumber">@item.SizeName</span>
                        ++tempIndex;
                    }
                </div>
                <div class="product-attribute product-count">
                    <span class="product-attribute-title">数量</span>
                    <span class="product-count-s product-count-cut">-</span>
                    <input class="product-count-s product-count-value" value="1" />
                    <span class="product-count-s product-count-add">+</span>
                </div>
                <div class="product-attribute product-rmb">
                    <span class="product-attribute-title">RMB：</span>
                    <span class="product-count-s product-rmbNum">@Model.TagPrice</span>
                </div>
                <div class="clearfix product-operation">
                    <div class="addBuyCar">加入购物车</div>
                    <div class="apppointment" onclick="Appointments()">预约</div>
                </div>
                <div class="swiper-container" id="swiper1">
                    <div class="swiper-wrapper">
                        @foreach (var item in listpro.Where(w => w.OriginalPath.IsNotNullAndEmpty()).DistinctBy(d => d.ColorId))
                        {
                            <div class="swiper-slide"><img src="@WebUrl@item.OriginalPath" data-ColorId="@item.ColorId" /></div>
                        }
                    </div>

                </div>
                <div class="swiper-button-prev swiper-button-white"></div>
                <div class="swiper-button-next swiper-button-white"></div>
            </div>
        </div>
        <div class="product-part2">
            <div class="clearfix product-part2-content">
                <div class="swiper-container" id="swiper2">
                    <div class="swiper-wrapper" fashion="img1">

                        @{
                            var listDetails = Model.ProdcutOrigNumberProductDetails.Where(w => w.IsEnabled && !w.IsDeleted && w.Text.IsNotNullAndEmpty());
                            var listDetailInfo = listDetails.Where(w => w.TemplateTypeFlag == Whiskey.ZeroStore.ERP.Models.TemplateTypeFlag.手机).ToList();
                            if (listDetailInfo.IsNull())
                            {
                                listDetailInfo = listDetails.Where(w => w.TemplateTypeFlag == Whiskey.ZeroStore.ERP.Models.TemplateTypeFlag.PC).ToList();
                            }

                            var shipai = listDetailInfo.Where(w => w.Mark == "img1").Select(s => s.Text).ToList();
                            var listshiapi = string.Join(",", shipai).Split(",", true).ToList();//实拍展示图片地址集合
                        }

                        @foreach (var item in listshiapi)
                        {
                            <div class="swiper-slide"><img src="@item" /></div>
                        }

                    </div>
                    @if (listshiapi.Count > 2)
                    {
                        <div class="swiper-button-prev"></div>
                        <div class="swiper-button-next"></div>
                    }
                </div>
            </div>
        </div>
        <div class="product-part3">
            <div class="part3-title productTitle">
                <img src="/Content/Images/product/part3-title.png" />
            </div>
            <div class="part-content">
                <div class="part3-ocontent part3-fabric" fashion="img2">
                    @{
                        var modimg2 = listDetailInfo.FirstOrDefault(w => w.Mark == "img2");
                        var img2 = modimg2?.Text ?? string.Empty;
                        var modtxt5 = listDetailInfo.FirstOrDefault(w => w.Mark == "txt5");
                        var txt5 = MvcHtmlString.Create(modtxt5?.Text ?? string.Empty);
                        var modtxt6 = listDetailInfo.FirstOrDefault(w => w.Mark == "txt6");
                        var txt6 = MvcHtmlString.Create(modtxt6?.Text ?? string.Empty);
                        var modtxt7 = listDetailInfo.FirstOrDefault(w => w.Mark == "txt7");
                        var txt7 = MvcHtmlString.Create(modtxt7?.Text ?? string.Empty);
                    }
                    <img src="@img2" />
                    <div class="fabric-1" fashion="txt5">@txt5</div>
                    <div class="part3-fabric-text">
                        <span class="point"></span>
                        <div class="line"></div>
                        <div class="fabric" fashion="txt7">@txt7</div>
                        <div class="fabricDes" fashion="txt6">@txt6</div>
                    </div>
                </div>
                <div class="part3-ocontent part3-thickness">
                    <img src="@img2" />
                    @{
                        var listPA = Model.ProductAttributes.Where(w => w.IsEnabled && !w.IsDeleted).ToList();
                        var mianliaoArrInt = new int[] { 312, 313, 314, 315, 316, 317, 318, 341, 342, 343 };
                        var listmianliao = listPA.Where(w => mianliaoArrInt.Contains(w.Id)).ToList();
                    }
                    <div class="part3-thickness-content">
                        @foreach (var item in listmianliao)
                        {
                            var strfabimg = string.Format("{0}{1}.png", "/Content/Images/product/fabric", item.Id);
                            <img class="fabricImage" src="@strfabimg" />
                        }
                    </div>
                </div>
            </div>
        </div>
        <div class="product-part4">
            <div class="part4-title">
                <img src="/Content/Images/product/part4-title.png" />
            </div>
            @{
                var modimg3 = listDetailInfo.FirstOrDefault(w => w.Mark == "img3");
                var img3 = modimg3?.Text ?? string.Empty;
                var modimg4 = listDetailInfo.FirstOrDefault(w => w.Mark == "img4");
                var img4 = modimg4?.Text ?? string.Empty;
                var modimg5 = listDetailInfo.FirstOrDefault(w => w.Mark == "img5");
                var img5 = modimg5?.Text ?? string.Empty;
                var modtxt1 = listDetailInfo.FirstOrDefault(w => w.Mark == "txt1");
                var txt1 = MvcHtmlString.Create(modtxt1?.Text ?? string.Empty);
                var modtxt2 = listDetailInfo.FirstOrDefault(w => w.Mark == "txt2");
                var txt2 = MvcHtmlString.Create(modtxt2?.Text ?? string.Empty);
                var modtxt3 = listDetailInfo.FirstOrDefault(w => w.Mark == "txt3");
                var txt3 = MvcHtmlString.Create(modtxt3?.Text ?? string.Empty);
            }
            <div class="product-part4-left">
                <div class="product-part4-left-Text">
                    <img class="product-part4-left-title" src="/Content/Images/product/part4-left-title.png" />
                    <div fashion="txt1">@txt1</div>
                </div>
                <img class="opacityBackground" fashion="img3" src="@img3" />
                <img class="ptoduct-part4-image" fashion="img3" src="@img3" />
            </div>
            <div class="product-part4-right">
                <div class="product-part4-right-Text">
                    <img class="product-part4-right-title" src="/Content/Images/product/part4-right-title.png" />
                    <div fashion="txt2">@txt2</div>
                </div>
                <img class="opacityBackground" fashion="img4" src="@img4" />
                <img class="ptoduct-part4-image" fashion="img4" src="@img4" />
            </div>
            <div class="product-part4-left">
                <div class="product-part4-left-Text">
                    <img class="product-part4-left-title" src="/Content/Images/product/part4-left-title.png" />
                    <div fashion="txt3">@txt3</div>
                </div>
                <img class="opacityBackground" fashion="img5" src="@img5" />
                <img class="ptoduct-part4-image" fashion="img5" src="@img5" />
            </div>
        </div>
        <div class="product-part5-title">

            <img src="/Content/Images/product/part5-title.png" />
        </div>
        <div class="product-part5">
            <div class="part5s"></div>
            <div class="clearfix ProductAttributesBox">
                @{
                    var attrimg = listPA.Where(w => w.Id >= 41 && w.Id <= 47).ToList();
                }
                @foreach (var item in attrimg)
                {
                    <div class="oProductAttributes"><img src="@WebUrl@item.IconPath" /><p class="oProductAttributesName">@item.AttributeName</p></div>
                }
            </div>
            <div class="clearfix product-part5-attribute-details">
                @{
                    var listyanse = listColorSizes.Select(s => s.ColorName).ToList();
                    var yanse = string.Join("/", listyanse).TrimEnd('/');
                    var listtixing = listPA.Where(w => w.Id >= 344 && w.Id <= 349).Select(s => s.AttributeName).ToList();
                    var tixing = string.Join("/", listtixing).TrimEnd('/');
                    var listcunzaigan = listPA.Where(w => w.Id >= 401 && w.Id <= 405).Select(s => s.AttributeName).ToList();
                    var cunzaigan = string.Join("/", listcunzaigan).TrimEnd('/');
                    var listchuanzhuofangshi = listPA.Where(w => w.Id >= 406 && w.Id <= 414).Select(s => s.AttributeName).ToList();
                    var chuanzhuofangshi = string.Join("/", listchuanzhuofangshi).TrimEnd('/');
                    var listxiaoguo = listPA.Where(w => w.Id >= 368 && w.Id <= 400).Select(s => s.AttributeName).ToList();
                    var xiaoguo = string.Join("/", listxiaoguo).TrimEnd('/');
                    var listfengge = listPA.Where(w => w.Id >= 41 && w.Id <= 47).Select(s => s.AttributeName).ToList();
                    var fengge = string.Join("/", listfengge).TrimEnd('/');
                    var listchanghe = listPA.Where(w => w.Id >= 48 && w.Id <= 54).Select(s => s.AttributeName).ToList();
                    var changhe = string.Join("/", listchanghe).TrimEnd('/');
                    var listtexing = listPA.Where(w => w.Id >= 350 && w.Id <= 361).Select(s => s.AttributeName).ToList();
                    var texing = string.Join("/", listtexing).TrimEnd('/');
                    var listjijie = listPA.Where(w => w.Id >= 362 && w.Id <= 367).Select(s => s.AttributeName).ToList();
                    var jijie = string.Join("/", listjijie).TrimEnd('/');
                }
                <div class="clearfix">
                    <div class="product-part5-attribute-section">
                        <span class="attribute-section-name">款式</span>
                        <span>@Model.Category.CategoryName</span>
                    </div>
                    <div class="product-part5-attribute-section">
                        <span class="attribute-section-name">颜色</span>
                        <span>@yanse</span>
                    </div>
                </div>
                <div class="clearfix">
                    <div class="product-part5-attribute-section">
                        <span class="attribute-section-name">季节</span>
                        <span>@jijie</span>
                    </div>
                    <div class="product-part5-attribute-section">
                        <span class="attribute-section-name">体型</span>
                        <span>@tixing</span>
                    </div>
                </div>
                <div class="clearfix">
                    <div class="product-part5-attribute-section">
                        <span class="attribute-section-name">效果</span>
                        <span>@xiaoguo</span>
                    </div>
                    <div class="product-part5-attribute-section">
                        <span class="attribute-section-name">风格</span>
                        <span>@fengge</span>
                    </div>
                </div>
                <div class="clearfix">
                    <div class="product-part5-attribute-section">
                        <span class="attribute-section-name">场合</span>
                        <span>@changhe</span>
                    </div>
                    <div class="product-part5-attribute-section">
                        <span class="attribute-section-name">特型</span>
                        <span>@texing</span>
                    </div>
                </div>
                <div class="clearfix">
                    <div class="product-part5-attribute-section">
                        <span class="attribute-section-name">存在感</span>
                        <span>@cunzaigan</span>
                    </div>
                    <div class="product-part5-attribute-section">
                        <span class="attribute-section-name">穿着方式</span>
                        <span>@chuanzhuofangshi</span>
                    </div>
                </div>
            </div>

        </div>
        <div class="product-part6">
            <div class="part6box">
                @{
                    var operaName = Model.Operator?.Member.MemberName ?? string.Empty;
                    var operaImg = Model.Operator?.Member.UserPhoto ?? string.Empty;
                    operaImg = operaImg.IsNotNullAndEmpty() ? WebUrl + operaImg : operaImg;
                }
                <img class="buyerImage" src="@operaImg" />
                <div class="buyerName">@operaName</div>
                <div class="buyertitle">买手说</div>
                <img class="buysayBackground" src="/Content/Images/product/buyersay.png" />
                @{
                    var modtxt4 = listDetailInfo.FirstOrDefault(w => w.Mark == "txt4");
                    var txt4 = modtxt4?.Text ?? string.Empty;
                }
                <div class="buyerSayBox" fashion="txt4">
                    @(MvcHtmlString.Create(txt4))
                </div>
            </div>
        </div>
        <div class="footer" onclick="location.href='/Home/Online'" style="cursor:pointer">0-FASHION</div>
    </div>

    <script src="/Content/Scripts/swiper/swiper.min.js"></script>
    <script src="/Content/Scripts/Jquery/jquery-1.11.1.min.js"></script>
    <script src="/Content/Scripts/Layer/layer.js"></script>
    <script>
        $(function () {
            var swiper1 = new Swiper('#swiper1', {
                pagination: '.swiper-pagination',
                slidesPerView: 2,
                paginationClickable: true,
                spaceBetween: 10,
                nextButton: '.swiper-button-next',
                prevButton: '.swiper-button-prev',
            });
            var swiper2 = new Swiper('#swiper2', {
                pagination: '.swiper-pagination',
                slidesPerView: 2,
                paginationClickable: true,
                spaceBetween: 50,
                nextButton: '.swiper-button-next',
                prevButton: '.swiper-button-prev',
            });

            var sizesArr = @Html.Raw(listcolorsizes);

            $(".product-color-image").click(function () {
                var colorid = $(this).attr("data-ColorId");
                var $sizelist = $(".product-sizeList");
                $sizelist.find(".product-osize").remove();

                var objSize = sizesArr.filter(function (item) {
                    return item.ColorId == colorid;
                });
                if (objSize) {
                    var strsizes = "";
                    $.each(objSize[0].Sizes,function (ind,item) {
                        var stractive = ind > 0 ? "" : "product-osize-avtive";
                        strsizes+="<span class='product-osize "+stractive+"' data-SizeId='"+item.SizeId+"' data-ProductId='"+item.ProductId+"' data-ProductNumber='"+item.ProductNumber+"'>"+item.SizeName+"</span>";
                    });
                    $sizelist.append(strsizes);
                }
            });

            $('body').on('click', '#swiper1 .swiper-slide', function () {
                $('.product-show-active').removeClass('product-show-active')
                $(this).addClass('product-show-active')
                $('.main-product-show').attr('src', $(this).find('img').attr('src'))
            })


            //---------------------------------------- color
            $('.product-show-box').on('click', '.product-color-image', function () {
                var colorId = $(this).attr('data-colorid');
                $('.product-color-image-active').removeClass('product-color-image-active');
                $(this).addClass('product-color-image-active');
                $('#swiper1 .swiper-slide img[data-colorid='+colorId+']').click()
            })

            //-------------------------------------------------size

            $('.product-show-box').on('click', '.product-osize', function () {
                $('.product-osize-avtive').removeClass('product-osize-avtive');
                $(this).addClass('product-osize-avtive')
            })

            //-----------------------------------------------count
            $('.product-count-add').click(function () {
                var this_count = parseInt($('.product-count-value').val());
                this_count++;
                $('.product-count-value').val(this_count);
            })
            $('.product-count-cut').click(function () {
                var this_count = parseInt($('.product-count-value').val());
                if (this_count <= 1) {
                    return;
                }
                this_count--;
                $('.product-count-value').val(this_count);
            })
            $('.product-count-value').keyup(function () {
                var this_count = parseInt($('.product-count-value').val());
                if (this_count < 1 || !this_count) {
                    this_count = 1;
                }
                $('.product-count-value').val(this_count);
            })

			//浏览10s后添加足迹
			var productnumber = getQueryObject(location.href).BigProdNum;
		
			setTimeout(function(){
				$.ajax({
					type:"post",
					data:{
						BigProNum:productnumber
					},
					url:"/Members/Member/AddBehaviorRecord",
					success:function(){}
				});
			},15000)

        })
        //--------------------------------------------------------解析URL
        function getQueryObject(url) {
        	url = url == null ? window.location.href : url;
        	var search = url.substring(url.lastIndexOf("?") + 1);
        	var obj = {};
        	var reg = /([^?&=]+)=([^?&=]*)/g;
        	search.replace(reg, function(rs, $1, $2) {
        		var name = decodeURIComponent($1);
        		var val = decodeURIComponent($2);
        		val = String(val);
        		obj[name] = val;
        		return rs;
        	});
        	return obj;
        }//--------------------------------------------------------预约
        function Appointments(){
            var productActive =  $('.product-osize-avtive');
            if( productActive.legth <= 0 ){
                layer.msg('请选择要预约的颜色和尺码')
                return;
            }
            var productid = parseInt(productActive.attr('data-productid'));
            var productnumber = productActive.attr('data-productnumber');
            $.ajax({
                url:'/Appointments/Appointment/Add',
                type:'post',
                data:{
                    data:[{ProductId:productid,ProductNumber:productnumber}]
                },
                success:function(data){
                    if(data.ResultType == 3 ){
                        layer.msg('预约成功',{icon:1})
                    }else if(data.ResultType == 4 ){
                        layer.msg(data.Message,{icon:0})
                    }else if(data.ResultType == 6 ){
                        layer.msg('您还未登录',{icon:0})
                    }else{
                        layer.msg(data.Message,{icon:2})
                    }
                }
            })
        }
    </script>

                    }
                    else
                    {
                        <title>零时尚 - 商品详情</title>
                        <style>
                            .notHasProduct {
                                position: fixed;
                                width: 100%;
                                height: 100%;
                                left: 0;
                                background: #382e2f;
                                color: #FFF;
                                text-align: center;
                                padding: 200px 0;
                            }

                                .notHasProduct img {
                                    width: 30px;
                                    margin: 0 20px;
                                    display: inline-block;
                                    vertical-align: middle;
                                }
                        </style>
                        <div class="notHasProduct" onclick="location.href='/Home/Online'" style="cursor:pointer"><img src="data:image/png;base64," />商品信息有误</div>
                    }
